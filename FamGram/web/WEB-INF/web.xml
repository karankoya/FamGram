<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.famgram.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>com.famgram.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JoinCommunity</servlet-name>
        <servlet-class>com.famgram.JoinCommunity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateCommunity</servlet-name>
        <servlet-class>com.famgram.CreateCommunity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadPic</servlet-name>
        <servlet-class>com.famgram.UploadPic</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCommunity</servlet-name>
        <servlet-class>com.famgram.DeleteCommunity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeaveCommunity</servlet-name>
        <servlet-class>com.famgram.LeaveCommunity</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JoinCommunity</servlet-name>
        <url-pattern>/JoinCommunity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateCommunity</servlet-name>
        <url-pattern>/CreateCommunity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadPic</servlet-name>
        <url-pattern>/UploadPic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCommunity</servlet-name>
        <url-pattern>/DeleteCommunity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeaveCommunity</servlet-name>
        <url-pattern>/LeaveCommunity</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
